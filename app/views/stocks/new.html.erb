<%= form_with model: @stock do |f| %>

     <%= f.label "Name:" %>
     <%= f.text_field :name,:readonly => true %>
     <%= f.label "Ticker:" %>
     <%= f.text_field :ticker,:readonly => true %>
     <%= f.label "Last Price:" %>
     <%= f.text_field :last_price,:readonly => true %>
     <%= f.label "Category:" %>
     <%= f.text_field :category %>
     
      <%= f.submit "Create" %>
     
  <% end %>